  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT d.month AS "Numer miesiąca", DATENAME(month, d.month * 29) AS Miesiąc, SUM(o.totalamount) AS [Suma sprzedaży]
FROM dates AS d INNER JOIN
    orders AS o ON d.date_id = o.orderdateid
GROUP BY d.month</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
