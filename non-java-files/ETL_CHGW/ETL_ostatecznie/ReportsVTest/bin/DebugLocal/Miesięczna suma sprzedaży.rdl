  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT DATENAME(month, d.month*29) AS "Miesiąc", SUM(o.totalamount) AS "Suma sprzedaży" FROM Dates d INNER JOIN Orders o ON d.date_id = o.orderdateid GROUP BY d.month</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
