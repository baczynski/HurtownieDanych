      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT c.country,d.month,COUNT(*)  FROM Customer c INNER JOIN Orders o ON c.customerid = o.customerid INNER JOIN Dates d ON d.date_id = o.orderdateid GROUP BY c.country,d.month</CommandText>
