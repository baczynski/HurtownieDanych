      <Fields>
        <Field Name="Numer_miesiąca">
          <DataField>Numer miesiąca</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Miesiąc">
          <DataField>Miesiąc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
